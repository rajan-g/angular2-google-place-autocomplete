{
  "name": "angular2-google-map-auto-complete",
  "version": "1.0.3",
  "description": "google map auto complete directive for angular2",
  "main": "index.js",
  "keywords": [
    "angular2-google-map-auto-complete",
    "angular2-google-place-auto-complete",
	"ng2-google-map-auto-complete",
    "ng2-google-place-auto-complete",
    "google-place-auto-complete"    
  ],
  "dependencies": {
    "angular2": "2.0.0-beta.12",
    "systemjs": "0.19.25",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
   "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings":"^0.8.1"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
